  "monitoring": {
    "enabled": true,
    "interval": 300000,
    "intervalDescription": "5 minutes",
    "autoSync": true,
    "autoTypeGeneration": true,
    "autoCommit": false,
    "enableNotifications": true
  },
  "notifications": {
    "push": true,
    "memory": true,
    "email": false,
    "webhook": false,
    "channels": {
      "memory": {
        "namespace": "schema-changes",
        "ttl": 86400000
      },
      "push": {
        "title": "Schema Change Detected",
        "urgency": "normal"
      }
    }
  },
  "validation": {
    "runTests": true,
    "checkTypeCompatibility": true,
    "validateRLS": true,
    "checkIndexes": true
  },
  "backup": {
    "enabled": true,
    "keepVersions": 10,
    "location": "./backups/schema-versions",
    "format": "typescript"
  },
  "dashboard": {
    "enabled": true,
    "port": 3001,
    "host": "localhost",
    "refreshInterval": 30000
  },
  "docker": {
    "required": true,
    "fallbackMode": true,
    "commands": {
      "pull": "supabase db pull",
      "diff": "supabase db diff",
      "generateTypes": "supabase gen types typescript --local",
      "status": "supabase status"
    }
  },
  "fallback": {
    "useJavaScriptClient": true,
    "queryInformationSchema": true,
    "compareWithCachedSchema": true
  },
  "ignore": {
    "tables": [
      "pg_*",
      "information_schema.*",
      "_realtime_*"
    ],
    "columns": [
      "updated_at"
    ],
    "functions": [
      "update_updated_at_column"
    ]
  },
  "lastSync": null,
  "lastSchemaHash": null,
  "version": "1.0.0"
